{
  "name": "ngssm-element",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build:ngssm-element": "ng build --project=ngssm-element",
    "test:ngssm-element": "ng test --no-watch --browsers=ChromeHeadless --project=ngssm-element --code-coverage",
    "build:element-provider": "ng build --project=element-provider && rm -rf ./src/assets/element-provider && mkdir -p ./src/assets/element-provider && cp -r ./dist/element-provider/browser/* ./src/assets/element-provider && mv ./src/assets/element-provider/main*js ./src/assets/element-provider/element-provider.js",
    "build:simple-element-provider": "ng build --project=simple-element-provider && rm -rf ./src/assets/simple-element-provider && mkdir -p ./src/assets/simple-element-provider && cp -r ./dist/simple-element-provider/browser/* ./src/assets/simple-element-provider && mv ./src/assets/simple-element-provider/main*js ./src/assets/simple-element-provider/simple-element-provider.js",
    "build:all": "npm run build:ngssm-element && npm run build:element-provider && npm run build:simple-element-provider"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~20.3.6",
    "@angular/cdk": "~20.2.9",
    "@angular/common": "~20.3.6",
    "@angular/compiler": "~20.3.6",
    "@angular/core": "~20.3.6",
    "@angular/elements": "~20.3.6",
    "@angular/forms": "~20.3.6",
    "@angular/material": "~20.2.9",
    "@angular/platform-browser": "~20.3.6",
    "@angular/platform-browser-dynamic": "~20.3.6",
    "@angular/router": "~20.3.6",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "angular-oauth2-oidc": "^20.0.2",
    "immutability-helper": "^3.1.1",
    "luxon": "^3.7.2",
    "ngssm-remote-data": "20.3.3",
    "ngssm-store": "20.3.3",
    "ngssm-toolkit": "20.3.3",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.6",
    "@angular/cli": "~20.3.6",
    "@angular/compiler-cli": "~20.3.6",
    "@types/jasmine": "~5.1.12",
    "@types/luxon": "^3.7.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "angular-eslint": "20.4.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jasmine-core": "~5.12.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "mkdirp": "^3.0.1",
    "ng-packagr": "^20.3.0",
    "ngssm-schematics": "20.3.3",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "typescript": "~5.9.3"
  }
}